[[profile]]
name = "dual"
exec = ["notify-send shikane \"Profile $SHIKANE_PROFILE_NAME has been applied\""]

    [[profile.output]]
    search  = "eDP-1" 
    enable = false

    [[profile.output]]
    search = "HDMI-A-1"
    enable = true
    mode = "2560x1440@59.95Hz"
    position = "0,0"
    scale = 1

    [[profile.output]]
    search  = "DP-1" 
    enable = true
    mode = "2560x1440@59.95Hz"
    position = "2560,0"
    #Transform = normal
    scale = 1

